{
  "name": "@nest-mods/auth",
  "version": "6.1.0",
  "description": "simple role based auth for nestjs using passport",
  "license": "MIT",
  "repository": "https://github.com/nest-mods/auth",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "author": {
    "name": "Diluka"
  },
  "scripts": {
    "test": "jest",
    "build": "tsc -p tsconfig.build.json",
    "clean": "rimraf dist",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "prepare": "npm run clean && npm run build"
  },
  "peerDependencies": {
    "@nestjs/common": ">=7",
    "@nestjs/core": ">=7"
  },
  "devDependencies": {
    "@nestjs/common": "^7.6.13",
    "@nestjs/core": "^7.6.13",
    "@nestjs/graphql": "^7.9.10",
    "@nestjs/platform-express": "^7.6.13",
    "@nestjs/testing": "^7.6.13",
    "@types/ioredis": "^4.22.0",
    "@types/jest": "^26.0.20",
    "@types/lodash": "^4.14.168",
    "@types/node": "^10.17.54",
    "@types/supertest": "^2.0.10",
    "@types/uuid": "^8.3.0",
    "apollo-server-express": "^2.21.0",
    "graphql": "^15.5.0",
    "graphql-tag": "^2.11.0",
    "graphql-tools": "^6.2.6",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.6",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.2",
    "ts-loader": "^8.0.17",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.2.2"
  },
  "dependencies": {
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "lodash": "^4.17.21",
    "passport": "^0.4.1",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.1.0",
    "uuid": "^8.3.2"
  },
  "engines": {
    "node": ">=10"
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all",
    "endOfLine": "lf"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".e2e-spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
